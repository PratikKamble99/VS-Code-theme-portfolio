# EmailJS Configuration for Terminal Message Sending
# Sign up at https://www.emailjs.com/ to get these values

# Your EmailJS Service ID
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_service_id_here

# Your EmailJS Template ID
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_template_id_here

# Your EmailJS Public Key
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key_here

# Alternative: Webhook Configuration (if not using EmailJS)
# NEXT_PUBLIC_WEBHOOK_URL=https://your-webhook-endpoint.com/api/messages
# NEXT_PUBLIC_WEBHOOK_SECRET=your_webhook_secret_here

# Rate Limiting Configuration
NEXT_PUBLIC_MESSAGE_COOLDOWN_MS=60000
